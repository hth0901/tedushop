<!DOCTYPE html>

<html>
<head>
    @*<meta name="viewport" content="width=device-width" />*@
    <title>Hihihaha</title>

    <link href="~/Assets/client/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Assets/client/font-awesome-4.7.0/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Assets/admin/libs/jquery-ui/themes/smoothness/jquery-ui.css" rel="stylesheet" />
    <link href="~/Assets/client/css/etalage.css" rel="stylesheet" />
    <!--theme-style-->
    <link href="~/Assets/client/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!--//theme-style-->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <script src="~/Assets/client/js/jquery.min.js"></script>
    <script src="~/Assets/client/js/jquery.etalage.min.js"></script>
    <script src="~/Assets/admin/libs/jquery-ui/jquery-ui.js"></script>

    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--fonts-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>
    <!--//fonts-->
    <!--script-->


    <script>
        jQuery(document).ready(function ($) {
            $('#etalage').etalage({
                thumb_image_width: 300,
                thumb_image_height: 400,
                source_image_width: 900,
                source_image_height: 1200,
                show_hint: true,
                click_callback: function (image_anchor, instance_id) {
                    alert('Callback example:\nYou clicked on an image with the anchor: "' + image_anchor + '"\n(in Etalage instance: "' + instance_id + '")');
                }
            });
        });
    </script>
</head>
<body>
    @{ 
        Html.RenderAction("Header", "Home");
    }
    <div class="container">
        @RenderBody()
        @{ 
            Html.RenderAction("Category", "Home");
        }
        <div class="clearfix"> </div>
    </div>
    <div class="clearfix"></div>
    <!---->

    <!---->
    @{ 
        Html.RenderAction("Footer", "Home");
    }
    <div class="clearfix"></div>
    
    
    <script src="~/Assets/client/js/common.js"></script>
    @RenderSection("footerJs", required: false)
</body>
</html>
